{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.9.0",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/test-login",
      "dest": "/api/test-login.php"
    },
    {
      "src": "/seed",
      "dest": "/api/seed.php"
    },
    {
      "src": "/debug",
      "dest": "/api/debug.php"
    },
    {
      "src": "/migrate",
      "dest": "/api/migrate.php"
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/storage/(.*)",
      "dest": "/public/storage/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",
  "env": {
    "APP_NAME": "SMK IT Ihsanul Fikri - Mungkid Magelang",
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_KEY": "base64:juQp72jOkqQ13Su+Dt02QPSIcj18elMVpqk+xxC4bEs=",
    "APP_URL": "https://edufikri-smk-system.vercel.app",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/tmp/database.sqlite",
    "DB_HOST": "",
    "DB_PORT": "",
    "DB_USERNAME": "",
    "DB_PASSWORD": "",
    "SESSION_DRIVER": "array",
    "CACHE_STORE": "array",
    "QUEUE_CONNECTION": "sync",
    "MAIL_MAILER": "log",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "debug"
  }
}